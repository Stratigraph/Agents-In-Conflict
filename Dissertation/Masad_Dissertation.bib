	keywords = {Experience weighted attraction, Fictitious play, Learning, Quantal response equilibrium, Reinforcement learning},
	pages = {177--198},
	file = {ScienceDirect Snapshot:/Users/dmasad/Library/Application Support/Zotero/Profiles/9ibei20q.default/zotero/storage/QFCXVFBN/S0022053106000056.html:text/html}
